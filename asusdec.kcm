#key BACKSLASH {
key BACKSLASH {
    #label:                              '\\'
    label:                              '\\'
    base:                               '\\'
    shift:                              '|'
}

#key D {
key SEMICOLON {
    #label:                              'D'
    #label:                              'xxx' 
    label:                              ';'
    base:                               'd'
    shift, capslock:                    'D'
}

#key Y {
key APOSTROPHE {
    #label:                              'Y'
   #label:                              'xxx' 
   label:                              '\''
   base:                               'y'
   shift, capslock:                    'Y'
}

### Numeric keypad ###

key NUMPAD_0 {
    #label:                              '0'
    label:                              '0'
    base:                               fallback INSERT
    numlock:                            '0'
}

key NUMPAD_1 {
    #label:                              '1'
    label:                              '1'
    base:                               fallback MOVE_END
    numlock:                            '1'
}

key NUMPAD_2 {
    #label:                              '2'
    label:                              '2'
    base:                               fallback DPAD_DOWN
    numlock:                            '2'
}

key NUMPAD_3 {
    #label:                              '3'
    label:                              '3'
    base:                               fallback PAGE_DOWN
    numlock:                            '3'
}

key NUMPAD_4 {
    #label:                              '4'
    label:                              '4'
    base:                               fallback DPAD_LEFT
    numlock:                            '4'
}

key NUMPAD_5 {
    #label:                              '5'
    label:                              '5'
    base:                               fallback DPAD_CENTER
    numlock:                            '5'
}

key NUMPAD_6 {
    #label:                              '6'
    label:                              '6'
    base:                               fallback DPAD_RIGHT
    numlock:                            '6'
}

key NUMPAD_7 {
    #label:                              '7'
    label:                              '7'
    base:                               fallback MOVE_HOME
    numlock:                            '7'
}

key NUMPAD_8 {
    #label:                              '8'
    label:                              '8'
    base:                               fallback DPAD_UP
    numlock:                            '8'
}

key NUMPAD_9 {
    #label:                              '9'
    label:                              '9'
    base:                               fallback PAGE_UP
    numlock:                            '9'
}

key NUMPAD_LEFT_PAREN {
    #label:                              '('
    label:                              '('
    base:                               '('
}

key NUMPAD_RIGHT_PAREN {
    #label:                              ')'
    label:                              ')'
    base:                               ')'
}

key NUMPAD_DIVIDE {
    #label:                              '/'
    label:                              '/'
    base:                               '/'
}

key NUMPAD_MULTIPLY {
    #label:                              '*'
    label:                              '*'
    base:                               '*'
}

key NUMPAD_SUBTRACT {
    #label:                              '-'
    label:                              '-'
    base:                               '-'
}

key NUMPAD_ADD {
    #label:                              '+'
    label:                              '+'
    base:                               '+'
}

key NUMPAD_DOT {
    #label:                              '.'
    label:                              '.'
    base:                               fallback FORWARD_DEL
    numlock:                            '.'
}

key NUMPAD_COMMA {
    #label:                              ','
    label:                              ','
    base:                               ','
}

key NUMPAD_EQUALS {
    #label:                              '='
    label:                              '='
    base:                               '='
}

key NUMPAD_ENTER {
    #label:                              '\n'
    label:                              '\n'
    base:                               '\n' fallback ENTER
    ctrl, alt, meta:                    none fallback ENTER
}

