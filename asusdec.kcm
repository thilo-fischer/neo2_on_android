#
# Key character map to use Neo2 ergonomic keyboard layout on ASUS
# Transformer Pad keyboards ("Mobile Docking").
#
# Desiged for and tested with ASUS Transformer Pad TF300T with Android 4.2.
#

type FULL

### Basic QWERTY keys ###

#key U {
key A {
    #label:                              'U'
    label:                              'A'
    base:                               'u'
#    shift, capslock:                    'U'

    shift: 't'
    alt: 'b'
    ctrl: 'd'
    meta: 'n'
    sym: 'Y'
    fn: 'F'
    numlock: 'N'
    capslock: 'L'
    scrolllock: 'R'

    lshift: 's'
    lalt: 'a'
    lctrl: 'c'
    lmeta: 'm'

    rshift: 'S'
    ralt: 'A'
    rctrl: 'C'
    rmeta: 'M'
}

#key Z {
