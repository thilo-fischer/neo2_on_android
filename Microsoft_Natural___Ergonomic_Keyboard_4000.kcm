#
# Key layout file to use the Neo2 ergonomic keyboard layout with "regular" keyboards.
#
# (Developed for and tested originally with Microsoft Natural Ergonomic Keyboard 4000.)
#
# This is yet only a pretty quick adaption of the layout I created for Asus Transformer keyboards.
# All basic keys work, but e.g. function keys (F1..F12) and any special keys will probably not work.
# TODO: Merge back stuff from Android's original Generic.k* files to get these things straight. 
#

type FULL

### Basic QWERTY keys ###

key U {
#key A {
    #label:                              'U'
    label:                              'A'
    base:                               'u'
    
    shift, capslock:                    'U'
    rshift, scrolllock:                 '\\'
    fn, numlock:                        fallback MOVE_HOME

#
# handy for testing:
#
#    shift: 't'
#    alt: 'b'
#    ctrl: 'd'
#    meta: 'n'
#    sym: 'Y'
#    fn: 'F'
#    numlock: 'N'
#    capslock: 'L'
#    scrolllock: 'R'
#
#    lshift: 's'
#    lalt: 'a'
#    lctrl: 'c'
#    lmeta: 'm'
#
#    rshift: 'S'
#    ralt: 'A'
#    rctrl: 'C'
#    rmeta: 'M'
#

}

key Z {
#key B {
    #label:                              'Z'
    label:                              'B'
    base:                               'z'
    shift, capslock:                    'Z'
    rshift, scrolllock:                 '`'
#    fn, numlock:                        fallback KEY_UNDO
}

key BUTTON_11 {
#key A_UMLAUT {
#key C {
    #label:                              '\u00c4'
    label:                              'C'
    base:                               '\u00e4'
    shift, capslock:                    '\u00c4'
    rshift, scrolllock:                 '|'
    fn, numlock:                        fallback INSERT
}

key A {
#key D {
    #label:                              'A'
    label:                              'D'
    base:                               'a'
    shift, capslock:                    'A'
    rshift, scrolllock:                 '{'
    fn, numlock:                        fallback DPAD_DOWN
}

key L {
#key E {
    #label:                              'L'
    label:                              'E'
    base:                               'l'
    shift, capslock:                    'L'
    rshift, scrolllock:                 '['
    fn, numlock:                        fallback DPAD_UP
}

key E {
#key F {
    #label:                              'E'
    label:                              'F'
    base:                               'e'
    shift, capslock:                    'E'
    rshift, scrolllock:                 '}'
    fn, numlock:                        fallback DPAD_RIGHT
}

key O {
#key G {
    #label:                              'O'
    label:                              'G'
    base:                               'o'
    shift, capslock:                    'O'
    rshift, scrolllock:                 '*'
    fn, numlock:                        fallback MOVE_END
}

key S {
#key H {
    #label:                              'S'
    label:                              'H'
    base:                               's'
    shift, capslock:                    'S'
    rshift, scrolllock:                 '?'
