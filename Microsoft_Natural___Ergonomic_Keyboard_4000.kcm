}

key G {
#key I {
    #label:                              'G'

    label:                              'I'
    base:                               'g'
    shift, capslock:                    'G'
    rshift, scrolllock:                 '>'
    fn, numlock:                        '8'
}

key N {
#key J {
    #label:                              'N'
    label:                              'J'
    base:                               'n'
    shift, capslock:                    'N'
    rshift, scrolllock:                 '('
    fn, numlock:                        '4'
}

key R {
#key K {
    #label:                              'R'
    label:                              'K'
    base:                               'r'
    shift, capslock:                    'R'
    rshift, scrolllock:                 ')'
    fn, numlock:                        '5'
}

key T {
#key L {
    #label:                              'T'
    label:                              'L'
    base:                               't'
    shift, capslock:                    'T'
    rshift, scrolllock:                 '-'
    fn, numlock:                        '6'
}

# M is the same for Neo and QWERTY
key M {
    label:                              'M'
    base:                               'm'
    shift, capslock:                    'M'
    rshift, scrolllock:                 '%'
    fn, numlock:                        '1'
}

key B {
#key N {
    #label:                              'B'
    label:                              'N'
    base:                               'b'
    shift, capslock:                    'B'
    rshift, scrolllock:                 '+'
    fn, numlock:                        ':'
}

key F {
#key O {
    #label:                              'F'
    label:                              'O'
    base:                               'f'
    shift, capslock:                    'F'
    rshift, scrolllock:                 '='
    fn, numlock:                        '9'
}

key Q {
#key P {
    #label:                              'Q'
    label:                              'P'
    base:                               'q'
    shift, capslock:                    'Q'
    rshift, scrolllock:                 '&'
