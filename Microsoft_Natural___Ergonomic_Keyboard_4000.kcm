}

key BUTTON_13 {
#key U_UMLAUT {
#key Z {
    #label:                              '\u00dc'
    #label:                              'Y'
    label:                              'Z'
    base:                               '\u00fc'
    shift, capslock:                    '\u00dc'
    rshift, scrolllock:                 '#'
    fn, numlock:                        '\u001b' fallback ESCAPE
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              '\u201d'
    rshift, scrolllock:                 '\u2019'
