}

key SPACE {
    label:                              ' '
    base:                               ' '
#    alt, meta:                          fallback SEARCH
#    ctrl:                               fallback LANGUAGE_SWITCH
    rshift, scrolllock:                 ' '
    fn, numlock:                        '0'
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
}

key TAB {
    label:                              '\t'
    base:                               '\t'
}

key COMMA {
    label:                              ','
    base:                               ','
    shift:                              '\u2013'
    rshift, scrolllock:                 '"'
    fn, numlock:                        '2'
}

key PERIOD {
    label:                              '.'
    base:                               '.'
    shift:                              '\u2022'
    rshift, scrolllock:                 '\''
