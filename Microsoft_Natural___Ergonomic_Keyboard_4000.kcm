}

key X {
#key Q {
    #label:                              'X'
    label:                              'Q'
    base:                               'x'
    shift, capslock:                    'X'
    rshift, scrolllock:                 '\u2026'
    fn, numlock:                        fallback PAGE_UP
}

key C {
#key R {
    #label:                              'C'
    label:                              'R'
    base:                               'c'
    shift, capslock:                    'C'
    rshift, scrolllock:                 ']'
    fn, numlock:                        fallback FORWARD_DEL
}

key I {
#key S {
    #label:                              'I'
    label:                              'S'
    base:                               'i'
    shift, capslock:                    'I'
    rshift, scrolllock:                 '/'
    fn, numlock:                        fallback DPAD_LEFT
}

key W {
#key T {
    #label:                              'W'
    label:                              'T'
    base:                               'w'
    shift, capslock:                    'W'
    rshift, scrolllock:                 '^'
    fn, numlock:                        fallback PAGE_DOWN
}

key H {
#key U {
    #label:                              'H'
    label:                              'U'
    base:                               'h'
    shift, capslock:                    'H'
    rshift, scrolllock:                 '<'
    fn, numlock:                        '7'
}

key P {
#key V {
    #label:                              'P'
    label:                              'V'
    base:                               'p'
    shift, capslock:                    'P'
    rshift, scrolllock:                 '~'
    fn, numlock:                        '\n'
#    fn, numlock:                        '\n' fallback ENTER
}

key V {
#key W {
    #label:                              'V'
    label:                              'W'
    base:                               'v'
    shift, capslock:                    'V'
    rshift, scrolllock:                 '_'
#    fn, numlock:                        '\u0008' fallback DEL
    fn, numlock:                        fallback DEL
}

key BUTTON_12 {
#key O_UMLAUT {
#key X {
    #label:                              '\u00d6'
    label:                              'X'
    base:                               '\u00f6'
    shift, capslock:                    '\u00d6'
    rshift, scrolllock:                 '$'
